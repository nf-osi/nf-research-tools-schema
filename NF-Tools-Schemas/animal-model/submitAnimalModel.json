{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Submit Animal Model",
  "type": "object",
  "properties": {
    "animalModelName": {
      "type": "string",
      "title": "What is the name of the animal model?"
    },
    "synonyms": {
      "type": "string",
      "title": "Are there any synonyms?"
    },
    "description": {
      "type": "string",
      "title": "Please provide a description of the animal model (~20 words)"
    },
    "backgroundStrain": {
      "type": "string",
      "title": "What is the background strain?"
    },
    "backgroundSubstrain": {
      "type": "string",
      "title": "What is the background substrain?"
    },
    "strainNomenclature": {
      "type": "string",
      "title": "What is the strain nomenclature?"
    },
    "animalModelDisease": {
      "type": "string",
      "title": "What is the animal model disease?",
      "enum": [
        "Neurofibromatosis type 1",
        "Neurofibromatosis type 2",
        "No known disease",
        "Schwannomatosis"
      ]
    },
    "animalModelManifestation": {
      "type": "string",
      "title": "What is the animal model of manifestation?",
      "enum": [
        "astrocytoma",
        "high grade glioma",
        "malignant peripheral nerve sheath tumor",
        "optic nerve glioma",
        "spinal development",
        "plexiform neurofibroma",
        "metabolic function",
        "heart malformation",
        "neural crest hyperplasia",
        "cognition",
        "growth",
        "acute myeloid leukemia",
        "no symptoms",
        "Other"
      ]
    },
    "mutationTypes": {
      "type": "array",
      "title": "Do any of the mutation types apply?",
      "items": {
        "type": "string",
        "enum": [
          "Disruption caused by insertion of vector",
          "Duplication",
          "Insertion",
          "Insertion of gene trap vector",
          "Intergenic deletion",
          "Intragenic deletion",
          "Inversion",
          "Loss of heterozygosity (unspecified mechanism)",
          "Nucleotide repeat expansion",
          "Nucleotide substitutions",
          "Single point mutation",
          "Translocation",
          "Transposon insertion",
          "Viral insertion",
          "Other"
        ]
      }
    },
    "alleleType": {
      "type": "string",
      "title": "What is the allele type?",
      "enum": [
        "Conditional ready",
        "Constitutively active",
        "Dominant negative",
        "Humanized sequence",
        "Hypomorph",
        "Inducible",
        "Inserted expressed sequence",
        "Knockdown",
        "Modified isoform(s)",
        "No functional change",
        "Null/knockout",
        "RMCE-Ready",
        "Recombinase",
        "Reporter",
        "Transactivator",
        "Transposase",
        "Transposon concatemer",
        "Other"
      ]
    },
    "mutationMethod": {
      "type": "string",
      "title": "What is the method mutation was applied?",
      "enum": [
        "Chemically and radiation induced",
        "Chemically induced (ENU)",
        "Chemically induced (other)",
        "Endonuclease-mediated (CRISPR)",
        "Endonuclease-mediated (TALEN)",
        "Gene trapped",
        "QTL",
        "Radiation induced",
        "Spontaneous",
        "Targeted",
        "Transgenic",
        "Transposon induced",
        "Other"
      ]
    },
    "affectedGeneSymbol": {
      "type": "string",
      "title": "What is the Affected Gene Symbol?"
    },
    "affectedGeneName": {
      "type": "string",
      "title": "What is the Affected Gene Name?"
    },
    "chromosomeNumber": {
      "type": "string",
      "title": "What chromosome number is the affected gene?"
    },
    "proteinVariation": {
      "type": "string",
      "title": "What is the protein variation?"
    },
    "sequenceVariation": {
      "type": "string",
      "title": "What is the sequence variation?"
    },
    "investigatorName": {
      "type": "string",
      "title": "What is the investigator's name who developed this tool?"
    },
    "investigatorAffiliation": {
      "type": "string",
      "title": "What is the investigator's affiliation who developed this tool?"
    },
    "publicationDOI": {
      "type": "string",
      "title": "If there is a publication associated with the development of this tool, please include the DOI link below."
    },
    "itemAcquisition": {
      "type": "string",
      "title": "How can one acquire this item?",
      "enum": [
        "Contact the Principal Investigator",
        "It's able to be purchased online",
        "Other"
      ]
    },
    "vendor": {
      "type": "string",
      "title": "Please select a vendor you are aware has this item."
    },
    "catalogNumber": {
      "type": "string",
      "title": "What is the catalog number?"
    },
    "catalogURL": {
      "type": "string",
      "title": "What is the catalog URL?"
    }
  },
  "required": [
    "animalModelName",
    "description",
    "backgroundStrain",
    "animalModelDisease",
    "animalModelManifestation",
    "investigatorName",
    "investigatorAffiliation"
  ]
}
