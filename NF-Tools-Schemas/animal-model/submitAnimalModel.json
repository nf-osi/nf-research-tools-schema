{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Submit Animal Model",
  "type": "object",
  "properties": {
    "animalModelName": {
      "type": "string",
      "title": "Name of Animal Model"
    },
    "synonyms": {
      "type": "string",
      "title": "Synonyms (if applicable)"
    },
    "description": {
      "type": "string",
      "title": "Description (~20 words)"
    },
    "backgroundStrain": {
      "type": "string",
      "title": "Background strain"
    },
    "backgroundSubstrain": {
      "type": "string",
      "title": "Background substrain"
    },
    "strainNomenclature": {
      "type": "string",
      "title": "Strain nomenclature"
    },
    "animalModelDisease": {
      "type": "string",
      "title": "Disease",
      "enum": [
        "Neurofibromatosis type 1",
        "Neurofibromatosis type 2",
        "No known disease",
        "Schwannomatosis"
      ]
    },
    "animalModelManifestation": {
      "type": "string",
      "title": "Manifestation",
      "enum": [
        "astrocytoma",
        "high grade glioma",
        "malignant peripheral nerve sheath tumor",
        "optic nerve glioma",
        "spinal development",
        "plexiform neurofibroma",
        "metabolic function",
        "heart malformation",
        "neural crest hyperplasia",
        "cognition",
        "growth",
        "acute myeloid leukemia",
        "no symptoms",
        "Other"
      ]
    },
    "mutationTypes": {
      "type": "array",
      "title": "Applied Mutations",
      "items": {
        "type": "string",
        "enum": [
          "Disruption caused by insertion of vector",
          "Duplication",
          "Insertion",
          "Insertion of gene trap vector",
          "Intergenic deletion",
          "Intragenic deletion",
          "Inversion",
          "Loss of heterozygosity (unspecified mechanism)",
          "Nucleotide repeat expansion",
          "Nucleotide substitutions",
          "Single point mutation",
          "Translocation",
          "Transposon insertion",
          "Viral insertion",
          "Other"
        ]
      }
    },
    "alleleType": {
      "type": "string",
      "title": "Allele Type",
      "enum": [
        "Conditional ready",
        "Constitutively active",
        "Dominant negative",
        "Humanized sequence",
        "Hypomorph",
        "Inducible",
        "Inserted expressed sequence",
        "Knockdown",
        "Modified isoform(s)",
        "No functional change",
        "Null/knockout",
        "RMCE-Ready",
        "Recombinase",
        "Reporter",
        "Transactivator",
        "Transposase",
        "Transposon concatemer",
        "Other"
      ]
    },
    "mutationTypes": {
      "type": "object",
      "title": "Mutation Types",
      "properties": {
        "Disruption caused by insertion of vector": { "type": "boolean" },
        "Duplication": { "type": "boolean" },
        "Insertion": { "type": "boolean" },
        "Insertion of gene trap vector": { "type": "boolean" },
        "Intergenic deletion": { "type": "boolean" },
        "Intragenic deletion": { "type": "boolean" },
        "Inversion": { "type": "boolean" },
        "Loss of heterozygosity (unspecified mechanism)": { "type": "boolean" },
        "Nucleotide repeat expansion": { "type": "boolean" },
        "Nucleotide substitutions": { "type": "boolean" },
        "Single point mutation": { "type": "boolean" },
        "Translocation": { "type": "boolean" },
        "Transposon insertion": { "type": "boolean" },
        "Viral insertion": { "type": "boolean" },
        "Other": { "type": "boolean" }
      }
    },
    "affectedGeneSymbol": {
      "type": "string",
      "title": "Affected Gene Symbol"
    },
    "proteinVariation": {
      "type": "string",
      "title": "Protein Variation"
    },
    "sequenceVariation": {
      "type": "string",
      "title": "Sequence Variation"
    },
    "investigatorName": {
      "type": "string",
      "title": "Developer Name"
    },
    "investigatorAffiliation": {
      "type": "string",
      "title": "Developer Affiliation"
    },
    "publicationDOI": {
      "type": "string",
      "title": "Publication Link (DOI)"
    },
    "itemAcquisition": {
      "type": "string",
      "title": "How to acquire",
      "enum": [
        "Contact the Principal Investigator",
        "It's able to be purchased online",
        "Other"
      ]
    },
    "vendor": {
      "type": "string",
      "title": "Please select a vendor you are aware has this item."
    },
    "catalogNumber": {
      "type": "string",
      "title": "What is the catalog number?"
    },
    "catalogURL": {
      "type": "string",
      "title": "What is the catalog URL?"
    }
  },
  "required": [
    "animalModelName",
    "description",
    "backgroundStrain",
    "animalModelDisease",
    "animalModelManifestation",
    "investigatorName",
    "investigatorAffiliation"
  ]
}
