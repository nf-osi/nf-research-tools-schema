{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Submit Cell Line",
  "type": "object",
  "properties": {
    "cellLineName": {
      "type": "string",
      "title": "What is the name of the cell line?",
      "description": "Provide the name of the cell line."
    },
    "synonyms": {
      "type": "string",
      "title": "Are there synonyms for the cell line's name?",
      "description": "Provide synonyms if available."
    },
    "originDescription": {
      "type": "string",
      "title": "Please describe the origin of the cell line in ~20 words."
    },
    "tissueOrigin": {
      "type": "string",
      "title": "What tissue does this cell line originate from?",
      "enum": [
        "Bone marrow",
        "Buccal mucosa",
        "Buffy coat",
        "Cerebral cortex",
        "Dorsal root ganglion",
        "Embryonic tissue",
        "Meninges",
        "Nerve tissue",
        "Optic nerve",
        "Plasma",
        "Primary tumor",
        "Sciatic nerve",
        "Serum",
        "Splenocyte",
        "Whole brain",
        "Blood",
        "Other"
      ]
    },
    "cancerType": {
      "type": "string",
      "title": "If this is a cancer cell, what type is it?",
      "enum": [
        "It's not a cancer cell",
        "Cancer cell line",
        "Conditionally immortalized cell line",
        "Embryonic stem cell",
        "Factor-dependent cell line",
        "Finite cell line",
        "Hybridoma",
        "Induced pluripotent stem cell",
        "Somatic stem cell",
        "Spontaneously immortalized cell line",
        "Stromal cell line",
        "Telomerase immortalized cell line",
        "Transformed cell line",
        "Undefined cell line type",
        "Other"
      ]
    },
    "geneticMutation": {
      "type": "string",
      "title": "Does this cell line have a genetic mutation?",
      "enum": [
        "Neurofibromatosis type 1",
        "Neurofibromatosis type 2",
        "Schwannomatosis",
        "Other"
      ]
    },
    "species": {
      "type": "string",
      "title": "What is the cell line's species?"
    },
    "donorAge": {
      "type": "string",
      "title": "What is the age of the donor?"
    },
    "donorSex": {
      "type": "string",
      "title": "What is the sex?",
      "enum": ["Male", "Female", "Other"]
    },
    "mutationType": {
      "type": "string",
      "title": "Do any of the mutation types apply?",
      "enum": [
        "Disruption caused by insertion of vector",
        "Duplication",
        "Insertion",
        "Insertion of gene trap vector",
        "Intergenic deletion",
        "Intragenic deletion",
        "Inversion",
        "Loss of heterozygosity (unspecified mechanism)",
        "Nucleotide repeat expansion",
        "Nucleotide substitutions",
        "Single point mutation",
        "Translocation",
        "Transposon insertion",
        "Viral insertion",
        "Other"
      ]
    },
    "alleleType": {
      "type": "string",
      "title": "What is the allele type?",
      "enum": [
        "Conditional ready",
        "Constitutively active",
        "Dominant negative",
        "Humanized sequence",
        "Hypomorph",
        "Inducible",
        "Inserted expressed sequence",
        "Knockdown",
        "Modified isoform(s)",
        "No functional change",
        "Null/knockout",
        "RMCE-Ready",
        "Recombinase",
        "Reporter",
        "Transactivator",
        "Transposase",
        "Transposon concatemer",
        "Other"
      ]
    },
    "mutationMethod": {
      "type": "string",
      "title": "What is the method mutation was applied?",
      "enum": [
        "Chemically and radiation induced",
        "Chemically induced (ENU)",
        "Chemically induced (other)",
        "Endonuclease-mediated (CRISPR)",
        "Endonuclease-mediated (TALEN)",
        "Gene trapped",
        "QTL",
        "Radiation induced",
        "Spontaneous",
        "Targeted",
        "Transgenic",
        "Transposon induced",
        "Other"
      ]
    },
    "affectedGeneSymbol": {
      "type": "string",
      "title": "What is the Affected Gene Symbol?"
    },
    "affectedGeneName": {
      "type": "string",
      "title": "What is the Affected Gene Name?"
    },
    "chromosomeNumber": {
      "type": "string",
      "title": "What chromosome number is the affected gene?"
    },
    "proteinVariation": {
      "type": "string",
      "title": "What is the protein variation?"
    },
    "sequenceVariation": {
      "type": "string",
      "title": "What is the sequence variation?"
    },
    "investigatorName": {
      "type": "string",
      "title": "What is the investigator's name who developed this tool?"
    },
    "investigatorAffiliation": {
      "type": "string",
      "title": "What is the investigator's affiliation who developed this tool?"
    },
    "publicationDOI": {
      "type": "string",
      "title": "If there is a publication associated with the development of this tool, please include the DOI link below."
    },
    "itemAcquisition": {
      "type": "string",
      "title": "How can one acquire this item?",
      "enum": [
        "Contact the Principal Investigator",
        "It's able to be purchased online",
        "Other"
      ]
    },
    "vendor": {
      "type": "string",
      "title": "Please select a vendor you are aware has this item."
    },
    "catalogNumber": {
      "type": "string",
      "title": "What is the catalog number?"
    },
    "catalogURL": {
      "type": "string",
      "title": "What is the catalog URL?"
    }
  },
  "required": [
    "cellLineName",
    "tissueOrigin",
    "cancerType",
    "species",
    "donorAge",
    "donorSex",
    "investigatorName",
    "investigatorAffiliation"
  ]
}
