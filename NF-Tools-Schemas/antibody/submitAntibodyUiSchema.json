{
  "antibodyName": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the name of the antibody"
  },
  "hostOrganism": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the host organism (e.g., rabbit, mouse)"
  },
  "isotype": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the isotype (e.g., IgG, IgM)"
  },
  "targetAntigen": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the target antigen"
  },
  "clonality": {
    "ui:widget": "select",
    "ui:placeholder": "Select the clonality",
    "ui:options": {
      "enumOptions": [
        {
          "label": "Monoclonal",
          "value": "Monoclonal"
        },
        {
          "label": "Polyclonal",
          "value": "Polyclonal"
        }
      ]
    }
  },
  "applications": {
    "ui:widget": "textarea",
    "ui:options": {
      "rows": 3
    },
    "ui:placeholder": "List the applications or uses separated by commas"
  },
  "dilutionFactor": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the dilution factor"
  },
  "availability": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the availability status (e.g., commercial, academic)"
  },
  "references": {
    "ui:widget": "textarea",
    "ui:options": {
      "rows": 3
    },
    "ui:placeholder": "Provide references (e.g., PubMed ID, DOI) separated by commas"
  },
  "fundingAgency": {
    "ui:widget": "text",
    "ui:placeholder": "Enter the funding agency"
  }
}
